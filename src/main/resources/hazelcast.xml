<hazelcast xsi:schemaLocation="http://www.hazelcast.com/schema/config hazelcast-config-3.4.xsd"
		   xmlns="http://www.hazelcast.com/schema/config"
		   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<map name="wordbook">
		<time-to-live-seconds>600</time-to-live-seconds>
		<max-size>200</max-size>
	</map>

	<map name="users">
		<time-to-live-seconds>600</time-to-live-seconds>
		<max-size>200</max-size>
	</map>

	<cache name="articleType">
		<eviction size="200"/>

		<statistics-enabled>true</statistics-enabled>
		<management-enabled>true</management-enabled>
	</cache>

	<cache name="userType">
		<eviction size="200"/>

		<statistics-enabled>true</statistics-enabled>
		<management-enabled>true</management-enabled>
	</cache>

	<cache name="teachType">
		<eviction size="200"/>

		<statistics-enabled>true</statistics-enabled>
		<management-enabled>true</management-enabled>
	</cache>

	<cache name="fourItemsType">
		<eviction size="200"/>

		<statistics-enabled>true</statistics-enabled>
		<management-enabled>true</management-enabled>
	</cache>

	<cache name="findTieInfo">
		<eviction size="200"/>

		<statistics-enabled>true</statistics-enabled>
		<management-enabled>true</management-enabled>
	</cache>

	<cache name="userAllInfo">
		<eviction size="200"/>

		<statistics-enabled>true</statistics-enabled>
		<management-enabled>true</management-enabled>
	</cache>

</hazelcast>