<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <classPathEntry  location="${class.path.entrty}"/>
<context id="MySqlTables" targetRuntime="MyBatis3">

    <jdbcConnection driverClass="${jdbc.driver}"
                    connectionURL="${jdbc.url}"
                    userId="${jdbc.user}"
                    password="${jdbc.password}">
    </jdbcConnection>

    <javaTypeResolver >
        <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>

    <javaModelGenerator targetPackage="com.cbis.model" targetProject="${dao.target.dir}">
        <property name="enableSubPackages" value="true" />
        <property name="trimStrings" value="true" />
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="com.cbis.mapping"  targetProject="${dao.target.dir}">
        <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>

    <javaClientGenerator type="XMLMAPPER" targetPackage="com.cbis.dao"  targetProject="${dao.target.dir}">
        <property name="enableSubPackages" value="true" />
    </javaClientGenerator>

    <table schema="CBIS" tableName="users" domainObjectName="UsersDto" >
    </table>

    <table schema="CBIS" tableName="groups" domainObjectName="GroupsDto" >
    </table>

    <table schema="CBIS" tableName="group_authorities" domainObjectName="GroupAuthoritiesDto" >
    </table>

    <table schema="CBIS" tableName="group_members" domainObjectName="GroupMembersDto" >
    </table>

    <table schema="CBIS" tableName="persistent_logins" domainObjectName="PersistentLoginsDto" >
    </table>


</context>
</generatorConfiguration>